{
  "kind": "ImageStream",
  "apiVersion": "v1",
  "metadata": {
    "name": "redhat-openjdk18-openshift",
    "annotations": {
      "openshift.io/display-name": "OpenJDK 8"
    }
  },
  "spec": {
    "dockerImageRepository": "registry.access.redhat.com/redhat-openjdk-18/openjdk18-openshift",
    "tags": [
      {
        "name": "latest",
        "annotations": {
          "description": "Build and run Java applications using Maven and OpenJDK 8. For more information about using this container image, see https://access.redhat.com/documentation/en-us/red_hat_jboss_middleware_for_openshift/3/html-single/red_hat_java_s2i_for_openshift/",
          "iconClass": "icon-jboss",
          "tags": "builder,java,xpaas,openjdk",
          "supports":"java:8,xpaas:1.0",
          "sampleRepo": "https://github.com/jboss-openshift/openshift-quickstarts",
          "sampleContextDir": "undertow-servlet",
          "version": "latest"
        }
      }
    ]
  }
}
